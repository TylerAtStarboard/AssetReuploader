{
    "name": "image-reuploader",
    "version": "1.0.0",
    "description": "A Node.js tool for reuploading and replacing images through Roblox's Open Cloud, with moderation checks.",
    "main": "src.js",
    "scripts": {
        "start": "node src.js",
        "build-exe": "pkg src.js --targets node16-win-x64 --output ImageUploader.exe"
    },
    "dependencies": {
        "follow-redirects": "^1.15.2",
        "form-data": "^4.0.0",
        "node-fetch": "^2.6.9"
    },
    "devDependencies": {
        "pkg": "^5.8.1"
    }
}
